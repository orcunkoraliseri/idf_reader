EnergyManagementSystem:Program,
    Door_Switch_Enabled_Thermostat_Reset_Program_S_LivingUnit, ! Name
    IF (S_Sliding_Door_Ventilation_Sensor > 0), ! open doors, actuate setpoints
      SET timesteps_after_doorshut_south = 0,
      SET S_LivingUnit_CLGSETP_Actuator = 32.22,
      SET S_LivingUnit_HTGSETP_Actuator = 12.78,
    ELSEIF S_LivingUnit_CLGSETP_Actuator > 24.4, ! doors are closed but were recently open
      SET timesteps_after_doorshut_south = timesteps_after_doorshut_south + 1,
      IF timesteps_after_doorshut_south == 1,
        SET S_LivingUnit_CLGSETP_Actuator = 30,
        SET S_LivingUnit_HTGSETP_Actuator = 14,
      ELSEIF timesteps_after_doorshut_south == 2,
        SET S_LivingUnit_CLGSETP_Actuator = 28,
        SET S_LivingUnit_HTGSETP_Actuator = 16,
      ELSEIF timesteps_after_doorshut_south == 3,
        SET S_LivingUnit_CLGSETP_Actuator = 26,
        SET S_LivingUnit_HTGSETP_Actuator = 18,
      ELSEIF timesteps_after_doorshut_south == 4,
        SET S_LivingUnit_CLGSETP_Actuator = 25,
        SET S_LivingUnit_HTGSETP_Actuator = 20,
        SET S_LivingUnit_CLGSETP_Actuator = null,
        SET S_LivingUnit_HTGSETP_Actuator = null,
      ENDIF,
    ELSE, ! doors are closed and haven't been open for at least the past 4 timesteps
      SET timesteps_after_doorshut_south = 0, ! re-initialize to zero
      SET S_LivingUnit_CLGSETP_Actuator = null,
      SET S_LivingUnit_HTGSETP_Actuator = null,
    ENDIF;

